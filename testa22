
CREATE TABLE [E_1]
( 
	[hello]              char(18)  NOT NULL 
)
go

ALTER TABLE [E_1]
	ADD CONSTRAINT [XPKE_1] PRIMARY KEY  CLUSTERED ([hello] ASC)
go

CREATE TABLE [good_morning]
( 
	[today]              char(18)  NOT NULL 
)
go

ALTER TABLE [good_morning]
	ADD CONSTRAINT [XPKgood_morning] PRIMARY KEY  CLUSTERED ([today] ASC)
go

CREATE TABLE [WEEK]
( 
	[sunday]             char(18)  NOT NULL 
)
go

ALTER TABLE [WEEK]
	ADD CONSTRAINT [XPKWEEK] PRIMARY KEY  CLUSTERED ([sunday] ASC)
go
